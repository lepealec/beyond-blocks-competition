>>> def pr(n):
	n=str(n)
	a=1
	b=2
	c=3
	d=4
	e=5
	f=0
	g=0
	while e!=len(n):
		if int(n[a])*int(n[b])*int(n[c])*int(n[d])*int(n[e])>g:
			g=int(n[a])*int(n[b])*int(n[c])*int(n[d])*int(n[e])
			a=a+1
			b=b+1
			c=c+1
			d=d+1
			e=e+1
		else:
			a=a+1
			b=b+1
			c=c+1
			d=d+1
			e=e+1
	return g

>>> pr()
40824
