>>> def primenumber(a):
	n=0
	c=0
	z=0
	while c!=a:
		if prime(n)==True:
			z=n
			n=n+1
			c=c+1
		else:
			n=n+1
	print z,c

>>> def prime(x):
	a=2
	if x==2:
		return True
	if x<2:
		return False
	while a!=x:
		if x%a==0:
			return False
			a=a+1
		else:
			a=a+1
	return True

>>> primenumber(10001)
104743 10001
